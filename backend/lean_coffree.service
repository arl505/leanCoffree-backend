# /etc/systemd/system/lean_coffree.service
[Unit]
Description=leanCoffree
After=syslog.target

[Service]
User=root
ExecStart=/bin/java -jar /writeToFolder/backend.jar --server.port=8085 --spring.datasource.url=jdbc:mysql://localhost:3306/PLACEHOLDER?user=root&useSSL=false&serverTimezone=UTC --spring.datasource.password=PLACEHOLDER
SuccessExitStatus=143

[Install]
WantedBy=multi-user.target